

<ul class="breadcrumb mt-2 mb-5">
    <li class="breadcrumb-item"><a href="/">HOME</a></li>
    <% if(breadcrumbObjects) {%>
      <% breadcrumbObjects.forEach( obj => { %>
        <li class="breadcrumb-item"><a href="<%= obj.href %>"><%= obj.path.toUpperCase() %></a></li>
      <% }) %>
    <% } %>

</ul>