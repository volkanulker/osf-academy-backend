<% if(wishObjects.length === 0 ) { %>
  <div class="row">
    <div class="mt-3">
      <p class="text-muted"><h2>There is no item in the wishlist.</h2></p>
    </div>
  </div>

<% } else { %> 

  <% wishObjects.forEach( wishObj => { %>
    <div product-id="<%= wishObj.productId %>" variant-id="<%= wishObj.variantId %>" class="wish-card">
      <div class="m-3">
        <div class="card rounded shadow-lg" style="width: 18rem">
          <img
            class="card-img-top"
            src="/images/<%= wishObj.image.link %>"
            alt="Card image cap"
          />
          <div class="card-body">
            <h5 class="card-title"><%= wishObj.name %></h5>
          </div>
          <ul class="list-group list-group-flush">
            <li class="list-group-item">Price: <%= wishObj.price %> &dollar;</li>
          </ul>
          <div class="card-body">
            <div class="row">
              <div class="col-8">
                <div onclick="addToCard(this)" class="btn btn-warning"><b>Add to Cart</b></div>
              </div>
              <div class="col-4">
                <div onclick="deleteItem(this)" class="btn btn-danger"><i class="fa-solid fa-trash"></i></div>
              </div>
            </div>
          </div>
          <div class="container h-100">
            <div class="row h-100 justify-content-center align-items-center">
              <p class="text-danger" id="error-message-<%= wishObj.productId %>"></p>
            </div>
          </div>
          <div class="container h-100">
            <div class="row h-100 justify-content-center align-items-center">
              <p class="text-success" id="success-message-<%= wishObj.productId %>"></p>
            </div>
          </div>
        </div>
      </div>
    </div>
  

  <% }) %>
<% } %>